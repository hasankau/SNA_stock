	<queryString><![CDATA[select barcode, name, unit_fixed_id, stock_id,
unit_price, cash_price, invoice_details.qty, invoice_details.buying_price, invoice_details.qty*invoice_details.buying_price,
(sub_total-invoice_details.qty*invoice_details.buying_price) as profit,
discount, sub_total, date from item
inner join invoice_details on item.item_code=invoice_details.Item_item_code
inner join stock on item.item_code=stock.item_item_code
inner join invoice on invoice.invoice_id=invoice_details.invoice_invoice_id
where invoice.date between $P{from} and $P{to}
group by stock.stock_id;]]></queryString>

