	<queryString><![CDATA[SELECT
     item.`item_code` AS item_item_code,
     item.`description` AS item_description,
     stock_return_note.`date` AS stock_return_note_date,
     stock_return_items.`qty` AS stock_return_items_qty,
     despatch_note.`sdn_no` AS despatch_note_sdn_no,
     item.`manufacturer` AS item_manufacturer,
     despatch_items.`qty` AS despatch_items_qty
FROM
     `stock_return_note` stock_return_note INNER JOIN `stock_return_items` stock_return_items ON stock_return_note.`id` = stock_return_items.`stock_return_note_id`
     INNER JOIN `item` item ON stock_return_items.`item_id` = item.`id`
     INNER JOIN `despatch_items` despatch_items ON item.`id` = despatch_items.`item_id`
     INNER JOIN `despatch_note` despatch_note_A ON despatch_items.`despatch_note_id` = despatch_note_A.`id`
     INNER JOIN `despatch_note` despatch_note_B ON despatch_items.`despatch_note_id` = despatch_note_B.`id`
     AND despatch_note_B.`id` = stock_return_note.`despatch_note_id`
     AND despatch_note_A.`id` = stock_return_note.`despatch_note_id`
     INNER JOIN `despatch_note` despatch_note ON stock_return_note.`despatch_note_id` = despatch_note.`id`
WHERE
     srn_no = $P{srnno}]]></queryString>

